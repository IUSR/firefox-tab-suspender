{
  "name": "Tab Suspender",
  "author": "Michalewicz Piotr",
  "version": "3.0.1",
  "description": "Native, lightweight and performant open source extension that reduce Firefox's memory usage.",
  "manifest_version": 2,
  "icons": {
    "48": "fox-48px.png",
    "96": "fox-96px.png"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "tabsuspender@michalewiczpiotr",
      "strict_min_version": "59.0"
    }
  },
  "browser_action": {
    "default_icon": {
      "32": "fox-popup-32px.png",
      "64": "fox-popup-64px.png",
      "96": "fox-popup-96px.png"
    },
    "default_title": "Tab Suspender",
    "default_popup": "popup.html"
  }
}
